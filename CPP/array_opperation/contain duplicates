#include<iostream>
using namespace std;
bool is_contain_duplicate(int arr[],int n){
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
    if(arr[i]==arr[i+1]){
    return true;
        }
    }
}

    return false;

}


int main(){
    cout<<"enter the size of the element"<<" ";
    int n;
    cin>>n;
    int arr[n];
    cout<<"enter the"<<" "<<n<<" "<<"elements"<<" ";
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
   if(is_contain_duplicate(arr,n)){
       cout<<"array contains duplicate element"<<" ";
   }
   else{
       cout<<"array do not contain duplicate element"<<" ";
   }

    }
    
