#include<iostream>
using namespace std;
int main(){
 
 
 int n,i=2;
 cout<<"enter the value of n";
 cin>>n;
 
 while (i<n){
     
     if(n%i==0){
         
       cout<<"is not prime  " ; 
     }                    //this divides the number means it is not prime 
     
    else {
    cout<<" prime number";
    }
    i=i+1;
 }
 
    
    
    
    
}
